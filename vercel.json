{
    "version" : 2,
    "builds" : [
        {
            "src": "backend/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**/*.jsx",
            "use": "@vercel/static-build"
        }
    ],
    "routes" : [
        {
            "src": "/api/(.*)",
            "dest": "backend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
}
